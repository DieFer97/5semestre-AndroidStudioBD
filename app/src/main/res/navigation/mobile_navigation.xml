<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_view_contacts">

    <fragment
        android:id="@+id/navigation_view_contacts"
        android:name="com.example.agenda_app.ui.viewcontacts.ViewContactsFragment"
        android:label="Ver Contactos"
        tools:layout="@layout/fragment_view_contacts" />

    <fragment
        android:id="@+id/navigation_add_contacts"
        android:name="com.example.agenda_app.ui.addcontacts.AddContactsFragment"
        android:label="Agregar Contactos"
        tools:layout="@layout/fragment_add_contacts" />

    <fragment
        android:id="@+id/navigation_delete_contacts"
        android:name="com.example.agenda_app.ui.deletecontacts.DeleteContactsFragment"
        android:label="Eliminar Contactos"
        tools:layout="@layout/fragment_delete_contacts" />
</navigation>